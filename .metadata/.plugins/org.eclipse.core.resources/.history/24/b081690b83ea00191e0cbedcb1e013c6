package com.yash.assignment;

import java.util.Arrays;
import java.util.List;

public class Series {

	
	public int oddNumberFromSeries(List<Integer> sList) {
	
		for(int i=0,j=i+1;i<sList.size();i=+1,j=j+1)
			if((i+1)-i !=5)
				return (i+1);
			else if((j+1)-j !=10)
				return (j+1);
		return 0;
	}
	public static void main(String[] args) {
		Series series=new Series();
				System.out.println(series.oddNumberFromSeries(Arrays.asList(3,12,8,19,13,32,18,42,23,52)));
}
}
