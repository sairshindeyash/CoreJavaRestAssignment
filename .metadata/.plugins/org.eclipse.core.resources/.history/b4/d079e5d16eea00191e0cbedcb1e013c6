package com.yash.assignment;

import java.util.Arrays;
import java.util.List;

public class SubList {

	public List<Integer> sublist(int startIndex, int endIndex, List<Integer> asList) {
		System.out.println(asList);
		System.out.println(asList.subList(startIndex, startIndex));
		return asList.subList(startIndex, startIndex);
	}

	public static void main(String[] args) {
		SubList list=new SubList();
		System.out.println(list.sublist( 5, 10, Arrays.asList(1,2,3,4,5,10,5,20,30,22,66,2)));
	}
}
